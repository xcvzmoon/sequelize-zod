{
  "name": "@xcvzmoon/sequelize-zod",
  "version": "0.0.1",
  "description": "Generate Zod schemas automatically from your Sequelize models",
  "keywords": [
    "model",
    "schema",
    "sequelize",
    "zod"
  ],
  "license": "ISC",
  "author": {
    "name": "Mon Albert Gamil",
    "email": "mrgamilmonalbert@gmail.com",
    "url": "https://github.com/xcvzmoon/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xcvzmoon/sequelize-zod"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "oxlint --type-aware",
    "format": "oxfmt && oxfmt --check",
    "dev": "tsx watch packages/index.ts",
    "build": "tsdown",
    "changelog:bump": "pnpm dlx changelogen@latest --bump",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "@types/node": "^25.1.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.3",
    "sequelize": "^6.37.7",
    "tsdown": "^0.20.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "zod": "^4.3.6"
  },
  "peerDependencies": {
    "sequelize": "^6.37.7",
    "zod": "^4.3.6"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@10.28.2"
}
